generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Turma_Professor {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  idTurmaDisc Int
  codTurma    String
  codDisc     String
  disciplina  String
  professor   String
  num_Alunos  Int
  coordenador String   @default("n√£o informado")
  dataCriacao DateTime @default(now())
}
